   "source": [
    "# Python program to triple all numbers of a given list of integers using map.\n",
    "nums = (1, 2, 3, 4, 5, 6, 7) \n",
    "print(\"Original list: \", nums)\n",
    "result = map(lambda x: x + x + x, nums) \n",
    "print(\"Triple of list numbers:\")\n",
    "print(list(result))"
   ]
  }
